{
  "name": "icebird",
  "version": "0.3.0",
  "description": "Apache Iceberg client for javascript",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "keywords": [
    "avro",
    "hyparquet",
    "iceberg",
    "parquet",
    "hyparquet",
    "table",
    "parser",
    "datalake"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/icebird.git"
  },
  "files": [
    "src",
    "types"
  ],
  "type": "module",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./src/*.js": {
      "types": "./types/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "scripts": {
    "build:types": "tsc -p ./tsconfig.build.json",
    "coverage": "vitest run --coverage --coverage.include=src",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "npm run build:types",
    "test": "vitest run"
  },
  "dependencies": {
    "hyparquet": "1.19.0",
    "hyparquet-compressors": "1.1.1",
    "hyparquet-writer": "0.6.1"
  },
  "devDependencies": {
    "@types/node": "24.6.2",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.36.0",
    "eslint-plugin-jsdoc": "60.7.1",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}

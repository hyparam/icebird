{
  "name": "icebird",
  "version": "0.2.0",
  "description": "Apache Iceberg client for javascript",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "keywords": [
    "avro",
    "hyparquet",
    "iceberg",
    "parquet",
    "hyparquet",
    "table",
    "parser",
    "datalake"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/icebird.git"
  },
  "files": [
    "src",
    "types"
  ],
  "type": "module",
  "types": "types/iceberg.d.ts",
  "exports": {
    ".": {
      "types": "./types/iceberg.d.ts",
      "import": "./src/iceberg.js"
    },
    "./src/*.js": {
      "types": "./types/*.d.ts",
      "import": "./src/*.js"
    }
  },
  "scripts": {
    "build:types": "tsc -p ./tsconfig.build.json",
    "coverage": "vitest run --coverage --coverage.include=src",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "npm run build:types",
    "test": "vitest run"
  },
  "dependencies": {
    "hyparquet": "1.12.1",
    "hyparquet-compressors": "1.1.1"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.1.2",
    "eslint": "9.25.1",
    "eslint-plugin-jsdoc": "50.6.10",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  }
}
